			private function validation():String 
			{
				ErrStr="";
				var TxtFlag:Boolean=true;
				if(Drp_ContractType.selectedIndex==0)
				{
					if(TxtFlag==true)
					{
						Drp_ContractType.setFocus();
						TxtFlag=false;
					}
					ErrStr=ErrStr+ "*   Select Contract Type \n";
				}
				if(Txt_ContarctName.text=="")
				{
					if(TxtFlag==true)
					{
						Txt_ContarctName.setFocus();
						TxtFlag=false;
					}
					ErrStr=ErrStr+ "*   Contract Name \n";
				}
				if(Hlp_ClientName.text=="")
				{
					if(TxtFlag==true)
					{
						Hlp_ClientName.setFocus();
						TxtFlag=false;
					}
					ErrStr=ErrStr+ "*   Client Name \n";
				}
//				if(Drp_SalesExecutive.selectedIndex==-1)
//				{
//					if(TxtFlag==true)
//					{
//						Drp_SalesExecutive.setFocus();
//						TxtFlag=false;
//					}
//					ErrStr=ErrStr+ "*   Select SalesExecutive \n";
//				}
				if(dt_StartDate.text=="")
				 {
					if(TxtFlag==true)
					{
						dt_StartDate.setFocus();
						TxtFlag=false;
					}
					ErrStr=ErrStr+ "*   Start Date Required \n"; 
				}
			  
				if(Drp_BookingLevel.selectedIndex==0)
				{
					if(TxtFlag==true)
					{
						Drp_BookingLevel.setFocus();
						TxtFlag=false;
					}
					ErrStr=ErrStr+ "*   Select Booking Level \n";
				} 
				 
				if(grd_Services.dataProvider==null)
				{
					if(TxtFlag==true)
					{ 
						TxtFlag=false;
					}
					ErrStr=ErrStr+ "*   Service Details Reqired \n";
				}
				if(Drp_ContractType.selectedIndex==1)
				{
					if((Grd_ContractTariffsApart._getGridSrc==null)||(Grd_ContractTariffsApart._getGridSrc.length==0))
					{
						if(TxtFlag==true)
						{ 
							TxtFlag=false;
						}
						ErrStr=ErrStr+ "*   Tariff Data are Required \n";
					}
				}
				else
				{
					if((Grd_ContractTariffsApart1._getGridSrc==null)||(Grd_ContractTariffsApart1._getGridSrc.length==0))
					{
						if(TxtFlag==true)
						{ 
							TxtFlag=false;
						}
						ErrStr=ErrStr+ "*   Tariff Data are Required \n";
					}
				}
				
				return ErrStr;
			} 
			protected function SCtrl_SaveHandler():void
