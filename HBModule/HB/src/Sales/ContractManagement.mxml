								RateInterval:Drp_RateInterval.selectedItem, 
								AgreementDate:dt_agreementDate.text,Id:Id,ClientId:ClientId,
								SalesExecutiveId:SalesExecutiveId,PrintingModel:'',
								Types:'',TransubName:'',TransubId:0,
								BookingLevel:Drp_BookingLevel.selectedItem
							//	BookingLevel:Dd1l_BookingLevel.selectedItem,Property:Hlp_Property.text,PropertyId:PropertyId,
							}]);
						
						var HdrXml:String = ObjToXmlStr_Comm(Ac,"HdrXml");
						//var TarrifXml:String="";
						//TarrifXml += "<ROOT>";
						//TarrifXml += ObjToXmlStr_Comm(Grd_ContractTariff._getGridSrc,"TarrifXml")
						//TarrifXml += "</ROOT>";
						if(Drp_ContractType.selectedIndex==1)
						{
							var TarrifApartXml:String="";
							TarrifApartXml += "<ROOT>";
							TarrifApartXml += ObjToXmlStr_Comm(Grd_ContractTariffsApart._getGridSrc  ,"TarrifApartXml")
							TarrifApartXml += "</ROOT>"; 
						}
						else
						{
							var TarrifApartXml:String="";
							TarrifApartXml += "<ROOT>";
							TarrifApartXml += ObjToXmlStr_Comm(Grd_ContractTariffsApart1._getGridSrc  ,"TarrifApartXml")
							TarrifApartXml += "</ROOT>";
						}
						
						var ServiceXml:String="";
						//grd_Services.validateNow();
						var arr:ArrayCollection= new  ArrayCollection();
						arr.removeAll();
						arr=grd_Services.dataProvider as ArrayCollection;
						ServiceXml += "<ROOT>";
						
						ServiceXml += ObjToXmlStr_Comm(arr,"ServiceXml")
						ServiceXml += "</ROOT>"; 
						useWeb("ContractManagement","Save_CUSTOM",[GlobalXml,HdrXml,TarrifApartXml,ServiceXml]);
					 
				}
