					return;
				} 
				else
				{ 
					var Ac:ArrayCollection=new ArrayCollection
							([{ ContractName: Txt_ContarctName.text,ContractType:Drp_ContractType.selectedItem,
								ClientName:Hlp_ClientName.text,SalesExecutive:"",
								ContractPriceMode:Drp_ContractPriceMode.selectedItem,
