                    <h4 class="fltclr">Filter by Name</h4>
                    <form id="pname" name="pname" action="" method="post">
                        <input type="text" class="form-control ac_input" id="propertyname" name="propertyname" placeholder="Property Name" value="@ViewBag.name" autocomplete="off">
                        <input type="submit" id="propertyname" name="submit" class="go" value="Go">
                    </form>
                </div>
            }
            @using (Html.BeginForm("FilterResult", "Search", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                @Html.AntiForgeryToken()
